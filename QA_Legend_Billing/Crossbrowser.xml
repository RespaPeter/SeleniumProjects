<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test thread-count="5" name="Test">
  <parameter name="browser" value="chrome"></parameter>
  <groups>
  <run>
  <include name="Regression"></include>
  <exclude name="Smoke"></exclude>
  </run>
  </groups>
    <classes>
    <class name="com.qalegend.testscript.LoginTest">
      	<methods>
      		<include name="verifyLogin"/>
      		<exclude name="rememberMe"/>
      		<exclude name="verifyLoginTitle"/>
      		 <exclude name="verify Invalid login"></exclude>
   
      	</methods>
      	
    </class>
          	<class name="com.qalegend.testscript.HomeTest">
      	<methods>
      
      	<include name="calculatorClick"></include>
      	<include name="posTerminal"></include>
      	</methods>
      	</class>
    </classes>
      
 
  </test> <!-- Test -->
</suite> <!-- Suite -->
